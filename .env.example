# RPC URLs
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
BOB_SEPOLIA_RPC_URL=https://testnet.rpc.gobob.xyz
BASE_RPC_URL=https://mainnet.base.org
BOB_RPC_URL=https://rpc.gobob.xyz

# Private Keys (NEVER commit actual keys - use a test wallet for development)
DEPLOYER_PRIVATE_KEY=your_private_key_here

# Etherscan API Keys (for contract verification)
BASESCAN_API_KEY=your_basescan_api_key
BOBSCAN_API_KEY=your_bobscan_api_key
INKSCAN_API_KEY=your_bobscan_api_key

# Supabase
# Get from: Settings â†’ API in Supabase Dashboard
# New format (recommended): sb_publishable_... and sb_secret_...
# Legacy format (deprecated): eyJ... JWT tokens
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_publishable_key  # sb_publishable_... or legacy anon key

# Service role key (backend scripts only - NEVER commit actual key!)
# Only needed for admin operations like backfill-nft-metadata.js
SUPABASE_SERVICE_ROLE_KEY=your_supabase_secret_key  # sb_secret_... or legacy service_role key

# WalletConnect
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Contract Addresses (filled after deployment)
# Testnet
VITE_JOURNAL_PROXY_BASE_SEPOLIA=
VITE_JOURNAL_PROXY_BOB_SEPOLIA=

# Mainnet
VITE_JOURNAL_PROXY_BASE=
VITE_JOURNAL_PROXY_BOB=

# Gnosis Safe Addresses (filled after setup)
GNOSIS_SAFE_BASE=
GNOSIS_SAFE_BOB=

# Timelock Addresses (filled after deployment)
TIMELOCK_BASE=
TIMELOCK_BOB=

# Frontend
VITE_APP_URL=http://localhost:3000
VITE_ENVIRONMENT=development # development | staging | production
